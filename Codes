package age;
import java.util.Scanner;
public class Age {
    
    public static void main(String[] args) 
    {
        
	System.out.println("Hello World!!!!"+"\nHow are You Programers!"+"\n\tWelcome to JAVA");
	
	Scanner kb = new Scanner(System.in);
	int age;
	System.out.print("Enter Your Age !");
	age = kb.nextInt();
	System.out.println("Your age is : "+ age);
    }
    
}









class Lamp {
  
  // stores the value for light
  // true if light is on
  // false if light is off
  boolean isOn;

  // method to turn on the light
  void turnOn() {
    isOn = true;
    System.out.println("Light on? " + isOn);

  }

  public static void main(String[] args) {
    
    // create an object of Lamp
    Lamp led = new Lamp();

    // access method using object
    led.turnOn();
  }
}








package Demo;
public class NewDemo
{
    void display ()
    {
        System.out.println ("Invoking Method");
    }
    
    public static void main (String[]args)
    {
        NewDemo obj = new NewDemo ();
        obj.display ();
    }
}



package week3;
import java.util.Scanner;
public class JavaSyntaxDemo
{
    public static void main(String[] args)
    {
        Scanner input=new Scanner(System.in);
        Scanner input1=new Scanner(System.in);
        String name;
        int N,R;
        boolean flag;
        char Ch;
        while(true)
        {
            flag=true;
        System.out.print("Enter your name :");
        name=input1.nextLine();
        System.out.print("Enter your programming grade : ");
        Ch=input1.nextLine().charAt(0);
        System.out.println("Hello "+name+" Enter a number : ");
        N=input.nextInt();
        
        for (int i=2 ; i<N ; i++)
        {
            if (N%i==0)
            {
                flag=false;
                break;  
            }
        }
        System.out.println("You got "+Ch+" Grade in programing ");
        if(flag=true)
            System.out.println("The number "+N+" is prime ");
        else

            System.out.println("The number "+N+" is not prime ");
        System.out.print("Do you want to repeat(1/0) ");
        R=input.nextInt();
        if(R==0)
            break;
    }
    
}
